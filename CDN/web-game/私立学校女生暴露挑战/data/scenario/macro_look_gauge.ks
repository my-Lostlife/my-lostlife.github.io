	[eval exp="tf.left = 670"]
	[eval exp="tf.top = 110"]
	
[elsif exp="f.mob == 'jk2'"]
	[eval exp="tf.left = 300"]
	[eval exp="tf.top = 110"]
[endif]

[chara_show name="look_gauge" time=0 left="& tf.left" top="& tf.top"]
