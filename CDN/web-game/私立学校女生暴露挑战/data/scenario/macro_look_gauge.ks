*look_gauge_N_500
[macro name=視線ゲージN_500]
[if exp="f.mob == 'man'"]
	[eval exp="tf.left = 700"]
	[eval exp="tf.top = 70"]
	
[elsif exp="f.mob == 'woman'"]
	[eval exp="tf.left = 270"]
	[eval exp="tf.top = 90"]
	
[elsif exp="f.mob == 'kid_b'"]
	[eval exp="tf.left = 270"]
	[eval exp="tf.top = 230"]
	
[elsif exp="f.mob == 'kid_g'"]
	[eval exp="tf.left = 700"]
	[eval exp="tf.top = 210"]
	
[elsif exp="f.mob == 'police'"]
	[eval exp="tf.left = 290"]
	[eval exp="tf.top = 70"]
	
[elsif exp="f.mob == 'jd1'"]
	[eval exp="tf.left = 260"]
	[eval exp="tf.top = 70"]
	
